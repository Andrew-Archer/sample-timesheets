<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        class="com.haulmont.timesheets.gui.commandline.CommandLineFrameController"
        messagesPack="com.haulmont.timesheets.gui.commandline">
    <companions>
        <web class="com.haulmont.timesheets.web.commandline.WebCommandLineCompanion"/>
    </companions>
    <actions>
        <action id="applyAction" invoke="apply"/>
    </actions>
    <layout>
        <hbox id="commandLineHBox" spacing="true">
            <button id="apply" action="applyAction" caption="msg://caption.apply"/>
        </hbox>
        <hbox>
            <label htmlEnabled="true" value="msg://caption.commandLine.help"/>
        </hbox>
    </layout>
</window>
