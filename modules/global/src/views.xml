<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/5.4/view.xsd">
    <view class="com.haulmont.timesheets.entity.Tag"
          name="tag-with-type">
        <property name="name"/>
        <property name="code"/>
        <property name="description"/>
        <property name="tagType"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.timesheets.entity.TagType"
          name="tagType-with-project">
        <property name="name"/>
        <property name="code"/>
        <property name="description"/>
        <property name="project"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.timesheets.entity.TimeEntry"
          name="timeEntry-full">
        <property name="task">
            <property name="name"/>
            <property name="project">
                <property name="name"/>
                <property name="client">
                    <property name="name"/>
                </property>
                <property name="participants">
                    <property name="user">
                        <property name="login"/>
                        <property name="name"/>
                    </property>
                    <property name="role">
                        <property name="name"/>
                    </property>
                </property>
            </property>
        </property>
        <property name="taskName"/>
        <property name="user"
                  view="_minimal"/>
        <property name="tags"
                  view="_minimal"/>
        <property name="date"/>
        <property name="time"/>
        <property name="status"/>
        <property name="description"/>
    </view>
    <view class="com.haulmont.timesheets.entity.Task"
          name="task-full">
        <property name="name"/>
        <property name="code"/>
        <property name="description"/>
        <property name="project"
                  view="_minimal">
            <property name="participants"
                      view="_minimal">
                <property name="user">
                    <property name="login"/>
                    <property name="name"/>
                    <property name="firstName"/>
                    <property name="lastName"/>
                </property>
                <property name="role">
                    <property name="name"/>
                </property>
            </property>
        </property>
        <property name="type"
                  view="_minimal"/>
        <property name="status"/>
        <property name="defaultTags">
            <property name="name"/>
            <property name="tagType">
                <property name="name"/>
            </property>
        </property>
        <property name="participants">
            <property name="user">
                <property name="login"/>
                <property name="name"/>
                <property name="position"/>
            </property>
            <property name="role"
                      view="_minimal"/>
        </property>
        <property name="requiredTagTypes"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.timesheets.entity.ProjectParticipant"
          name="projectParticipant-full">
        <property name="user"
                  view="_local"/>
        <property name="project">
            <property name="name"/>
            <property name="participants">
                <property name="user">
                    <property name="login"/>
                    <property name="name"/>
                </property>
            </property>
        </property>
        <property name="role"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.timesheets.entity.Project"
          name="project-full">
        <property name="name"/>
        <property name="code"/>
        <property name="parent"
                  view="_minimal"/>
        <property name="client"
                  view="_minimal"/>
        <property name="description"/>
        <property name="status"/>
        <property name="participants">
            <property name="user">
                <property name="login"/>
                <property name="name"/>
                <property name="firstName"/>
                <property name="lastName"/>
                <property name="position"/>
            </property>
            <property name="role"
                      view="_minimal"/>
        </property>
        <property name="tasks"
                  view="task-full"/>
    </view>
    <view class="com.haulmont.timesheets.entity.Project"
          name="project-with-parent">
        <property name="name"/>
        <property name="code"/>
        <property name="parent"
                  view="_minimal"/>
        <property name="description"/>
        <property name="status"/>
    </view>
</views>
